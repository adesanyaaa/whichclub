<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="false">
  <local-check-config name="Saner Sun Checks (Eclipse)" location="checkstyle.xml" type="project" description="Slightly modified version of Sun Checks (Eclipse) that better matches the reality of coding.">
    <additional-data name="protect-config-file" value="false"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="Saner Sun Checks (Eclipse)" local="true">
    <file-match-pattern match-pattern="src/." include-pattern="true"/>
  </fileset>
</fileset-config>
